<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
    </startup>
  <system.serviceModel>
    <services>
      <service name="ServiceBusDemo.DemoManager">
        <endpoint address="sb://denverdevdaytest.servicebus.windows.net/DemoService" binding="netTcpRelayBinding" contract="ServiceBusDemo.IDemoService"/>
        <endpoint binding="netTcpBinding" contract="ServiceBusDemo.IDemoService"/>
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:8888/DemoManager"/>
            <add baseAddress="http://localhost:9988/DemoManager"/>
