        <binding transactionFlow="true" maxReceivedMessageSize="2000000">
          <reliableSession enabled="true"/>
        </binding>
      </netTcpBinding>
      <netTcpRelayBinding>
        <binding maxReceivedMessageSize="2000000">
          <reliableSession enabled="true"/>
        </binding>
      </netTcpRelayBinding>
